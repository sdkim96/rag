    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Entrypoint",
            "type": "debugpy",
            "request": "launch",
            "module": "pyagents",
            
        },
        {
            "name": "Python Debugger: ControlPlane",
            "type": "debugpy",
            "request": "launch",
            "module": "pyagents.control_plane",
            
        },
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
        },
        {
            "name": "Launch file",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${file}"
        },
        {
            "name": "Go gin서버 실행",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/backend/cmd/main.go",
        }
    ]
}